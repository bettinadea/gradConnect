<!doctype html>

<html>
<head>
	<title>Grad Connect</title></br></br>
	<link rel="stylesheet" href="css/bootstrap-theme.css" />
	<link rel="stylesheet" href="css/bootstrap.css" />
	<link rel="stylesheet" href="css/clinicalgrads.css" />
</head>

<form action="/">
	<input type="submit" value="Home Page">
</form>

<body>
	<center>
		<div class="jumbotron">
			<h1>Clinical Cognition Graduates</h1>
		</div>
		{{#each graduates}}
			<div class="graduate" id="{{id}}">
				<a href="#" class="thumbnail">
					<img src="{{imageURL}}" class="img-responsive"></img>
					<h3>{{name}}</h3>
					<p>{{description}}</p>
				</a>
			</div>
		{{/each}}
	</center>

	<script src="js/jquery-1.11.0.js" />
	<script src="js/bootstrap.js" />
	<script src="js/professor.js" />
	<script src="routes/clinicalgrads.js" />
</body>
</html>